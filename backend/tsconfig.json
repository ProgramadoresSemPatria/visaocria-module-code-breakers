{
	"compilerOptions": {
	  "target": "ES2020",
	  "module": "CommonJS",
	  "rootDir": "./",        // Define o rootDir como o diretório raiz
	  "outDir": "./dist",     // Saída para "dist"
	  "strict": true,
	  "esModuleInterop": true
	},
	"include": ["src/**/*", "models/src/**/*"], // Inclui "models" explicitamente
	"exclude": ["node_modules"]
  }